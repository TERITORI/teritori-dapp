{
  "extends": "expo/tsconfig.base",
  "target": "ES5",
  "compilerOptions": {
    "useDefineForClassFields": false,
    "strict": true,
    "removeComments": false,
    "target": "es6",
    "module": "esnext",
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "packages/*"
      ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts"
  ]
}
