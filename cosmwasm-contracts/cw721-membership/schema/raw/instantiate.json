{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "InstantiateMsg",
  "type": "object",
  "required": [
    "admin_addr",
    "description",
    "image_uri",
    "mint_royalties",
    "name",
    "symbol"
  ],
  "properties": {
    "admin_addr": {
      "type": "string"
    },
    "description": {
      "type": "string"
    },
    "image_uri": {
      "type": "string"
    },
    "mint_royalties": {
      "type": "integer",
      "format": "uint16",
      "minimum": 0.0
    },
    "name": {
      "type": "string"
    },
    "symbol": {
      "type": "string"
    }
  }
}
